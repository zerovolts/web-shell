font-stack = 'Inconsolata', monospace

body
  padding 0rem
  margin 0rem
  background #283646

.shell
  position absolute
  padding 1rem
  top 0px
  left 0px
  right 0px
  bottom 0px

.shell:focus
  outline none

.shell-line
  line-height 0.6rem
  font-size 1rem
  font-family font-stack
  color #eee

span.cursor
  background #eee
  color #283646
  //border-left 1px solid #eee
